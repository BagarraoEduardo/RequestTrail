{
  "openapi": "3.0.1",
  "info": {
    "title": "CalledApi",
    "version": "1.0"
  },
  "paths": {
    "/api/Main/GetExample": {
      "get": {
        "tags": [
          "Main"
        ],
        "operationId": "GetExample",
        "parameters": [
          {
            "name": "error",
            "in": "query",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessageDTO"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessageDTO"
                }
              }
            }
          }
        }
      }
    },
    "/api/Main/PostExample": {
      "post": {
        "tags": [
          "Main"
        ],
        "operationId": "PostExample",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BaseRequestDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/BaseRequestDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/BaseRequestDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseResponseDTO"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessageDTO"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorMessageDTO"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BaseRequestDTO": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "error": {
            "type": "boolean"
          }
        }
      },
      "BaseResponseDTO": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "message": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "ErrorMessageDTO": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "message": {
            "type": "string",
            "nullable": true
          }
        }
      }
    }
  }
}